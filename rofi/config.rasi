/* The default configuration incase none is specified */
configuration {
	width: 600;
	lines: 5;
	show-icons: true;
	terminal: "alacritty";
	auto-select: false;
	theme: "photon-black";
	display-run: " ";
	display-drun: "  ";
	display-window: "  ";
	drun-display-format: "{icon} {name}";
	font: "Source Code Pro for Powerline Medium 11";
	modi: "window,run,drun";
	show-icons: true;
}

@import "photon-black"

